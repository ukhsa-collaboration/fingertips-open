@using Fpm.MainUI
@model Fpm.MainUI.Models.LookupModel
@{ Layout = "~/Views/Shared/_LayoutPage.cshtml"; }
@{
    ViewBag.Title = "FPM - Lookup tables";
}
<link href="@(AppConfig.CssPath)TableSorter.css" rel="stylesheet" type="text/css" />
<script src="@(AppConfig.JsPath)lookup-tables.js" type="text/javascript"></script>

<div class="standardWidth">
    <h2 class="subheading">Lookup tables - @Model.LookupType</h2>
    @Html.Partial("_LookupSidebar")
    <div class="clearfix" style="float:left;">
        @Html.Partial("_Categories")
        @Html.Partial("_Sexes")
        @Html.Partial("_Ages")
        @Html.Partial("_ValueNotes")
        @Html.Partial("_Targets")
    </div>
</div>
