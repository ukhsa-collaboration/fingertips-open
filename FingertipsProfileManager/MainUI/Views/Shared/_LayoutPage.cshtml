@using Fpm.MainUI
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    
    <link rel="icon" href="/favicon.png" type="image/png">
    <link rel="Stylesheet" type="text/css" href="@(AppConfig.CssPath)bootstrap" />
    <link rel="Stylesheet" type="text/css" href="@(AppConfig.CssPath)fingertips.css" />
    <link rel="stylesheet" type="text/css" href="@(AppConfig.CssPath)dashboard.css" />
    <link rel="Stylesheet" type="text/css" href="@(AppConfig.CssPath)profile-manager.css" />

    <script src="@(AppConfig.JsPath)jquery-1.12.1.js" type="text/javascript"></script>
    <script src="@(AppConfig.JsPath)jquery-migrate-1.3.0.js" type="text/javascript"></script>
    <script src="@(AppConfig.JsPath)jquery.tablesorter.js" type="text/javascript"></script>
    <script src="@(AppConfig.JsPath)jquery.tablesorter.widgets.js" type="text/javascript"></script>
    <script src="@(AppConfig.JsPath)jquery-ui-1.9.2.js" type="text/javascript"></script>
    <script src="@(AppConfig.JsPath)jquery.validate.min.js" type="text/javascript"></script>
    <script src="@(AppConfig.JsPath)jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
    <script src="@(AppConfig.JsPath)underscore-min.js" type="text/javascript"></script>
    <script src="@(AppConfig.JsPath)modernizr-2.6.2.min.js" type="text/javascript"></script>
    <script src="@(AppConfig.JsPath)included-on-all-pages.js" type="text/javascript"></script>
    <script src="@(AppConfig.JsPath)bootstrap.js" type="text/javascript"></script>
    @RenderSection("ExtraScripts", required:false)
</head>
<body>
    @Html.Partial("_Banner")
    <div id="main" class="clearfix">
        @Html.Partial("_LightboxAndInfobox")
        @RenderBody()
    </div>
    @Html.Partial("_Footer")
    @RenderSection("AngularApp", required:false);
</body>
</html>
